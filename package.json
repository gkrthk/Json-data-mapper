{
  "name": "json-data-mapper",
  "version": "1.0.0",
  "description": "Json Mapper Utility to transform Json to required format based on the schema given.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": ["json-mapper","json-adapter","json-transform","typescript","transform", "json", "mapper","format","node","response-mapper","parse","schema"],
  "author": "gkrthk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gkrthk/Json-data-mapper.git"
  },
  "bugs": {
    "url":"https://github.com/gkrthk/Json-data-mapper/issues"
  },  
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "files": [
    "lib/**/*"
  ]
}
